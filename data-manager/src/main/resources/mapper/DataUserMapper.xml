<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.data.mapper.UserMapper">

    <insert id="insertToDataUser" parameterType="com.manager.common.core.domain.entity.DataUser">
        insert into data_user (phone,password) values (#{phone},#{password})
    </insert>

    <select id="loadDataUserName" parameterType="com.manager.common.core.domain.entity.DataUser" resultType="Integer">
        select count(phone) from data_user where phone=#{phone} and password = #{password}
    </select>
</mapper>
