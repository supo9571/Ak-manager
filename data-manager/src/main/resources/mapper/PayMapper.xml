<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.data.mapper.PayMapper">

    <insert id="saveBankReg">
        insert into config_recharge_order (order_number,uid,recharge_amount,recharge_type,payment_status,
                                           channel,reality_name,create_time,commodity_name)
        values (#{orderId},#{uid},#{money},2,0,#{channel},#{name},sysdate(),#{money})
    </insert>

</mapper>
