<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.consumer.mapper.DataMapper">

	<insert id="insertMsg">
		insert into sys_msg (`key`,`op`,`msg`)
		values (#{key},#{op},#{msg})
	</insert>
</mapper>
