<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.consumer.mapper.DataMapper">

	<update id="updateUserOnline">
		update  online_player
		set online_count = #{value},
			 time = #{time}
		where #{time} > time
	</update>

	<insert id="insertMsg">
		insert into sys_msg (`key`,`op`,`msg`)
		values (#{key},#{op},#{msg})
	</insert>
</mapper>
